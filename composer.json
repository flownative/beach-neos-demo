{
    "name": "flownative/beach-neos-demo-distribution",
    "type": "project",
    "description": "Neos Demo Distribution for Flownative Beach",
    "license": "GPL-3.0-or-later",
    "support": {
        "email": "support@flownative.com",
        "source": "https://github.com/flownative/beach-neos-demo"
    },
    "config": {
        "vendor-dir": "Packages/Libraries",
        "bin-dir": "bin",
        "allow-plugins": {
            "neos/composer-plugin": true
        }
    },
    "require": {
        "neos/neos": "^9.0",
        "neos/contentgraph-doctrinedbaladapter": "^9.0",
        "neos/neos-ui": "^9.0",
        "neos/seo": "~4.1",
        "neos/demo": "^9.0",
        "neos/setup": "@dev",
        "rokka/imagine-vips": "0.*",
        "flownative/google-cloudstorage": "6.*"
    },
    "require-dev": {
        "neos/buildessentials": "~9.0.x-dev",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^9.0",
        "neos/neos-setup": "^4.0.x-dev"
    },
    "repositories": {
        "distributionPackages": {
            "type": "path",
            "url": "./DistributionPackages/*"
        }
    },
    "allow-plugins": {
        "neos/composer-plugin": true,
        "composer/package-versions-deprecated": true
    },
    "scripts": {
        "post-create-project-cmd": "./flow welcome",
        "post-update-cmd": "Neos\\Flow\\Composer\\InstallerScripts::postUpdateAndInstall",
        "post-install-cmd": "Neos\\Flow\\Composer\\InstallerScripts::postUpdateAndInstall",
        "post-package-update": "Neos\\Flow\\Composer\\InstallerScripts::postPackageUpdateAndInstall",
        "post-package-install": "Neos\\Flow\\Composer\\InstallerScripts::postPackageUpdateAndInstall"
    }
}
